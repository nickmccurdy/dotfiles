!settings
urxvt.iconFile:    /usr/share/icons/Faenza/apps/16/terminal.png
urxvt.scrollstyle: plain
urxvt.visualBell:  true

!extensions
urxvt.perl-ext-common: default,tabbed,clipboard,matcher
URxvt.urlLauncher:     /usr/bin/firefox
URxvt.matcher.button:  1

!fonts
xft.antialias: false
urxvt.font:    xft:tamsyn:pixelsize=12

!main colors
urxvt.background:  #1e1e1e
urxvt.foreground:  white
urxvt.cursorColor: #00ff00

!additional colors
urxvt.color0:  black
urxvt.color1:  #cc0000
urxvt.color2:  #4e9a06
urxvt.color3:  #c4a000
urxvt.color4:  #3465a4
urxvt.color5:  #75507b
urxvt.color6:  #06989a
urxvt.color7:  #d3d7cf
urxvt.color8:  #555753
urxvt.color9:  #ef2929
urxvt.color10: #8ae234
urxvt.color11: #fce94f
urxvt.color12: #729fcf
urxvt.color13: #ad7fa8
urxvt.color14: #34e2e2
urxvt.color15: #d4d4d4

!tab colors
URxvt.tabbed.tabbar-fg: 0
URxvt.tabbed.tabbar-bg: 8
URxvt.tabbed.tab-fg:    0
URxvt.tabbed.tab-bg:    4
